{
  "name": "pipe-with",
  "version": "0.1.1",
  "description": "choose how to glue a pipeline of functions together (and keep type information)",
  "license": "MIT",
  "repository": "justinvdm/pipe-with",
  "author": {
    "name": "justinvdm",
    "email": "justnvdm@gmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "npm run lint && npm run typecheck && npm run unit",
    "unit": "ava",
    "typecheck": "flow --color always",
    "lint": "standard './**/*.js' './**/*.js.flow'",
    "release": "np"
  },
  "files": [
    "index.js",
    "index.js.flow"
  ],
  "keywords": [
    "pipe",
    "compose",
    "monads",
    "flow"
  ],
  "devDependencies": {
    "np": ">=3.0.2",
    "ava": ">=0.25.0",
    "babel-eslint": ">=10.0.1",
    "flow-bin": ">=0.86.0",
    "prettier-standard": ">=8.0.1",
    "standard": ">=12.0.1",
    "eslint-plugin-flowtype": ">=3.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
